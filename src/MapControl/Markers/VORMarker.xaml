<UserControl x:Class="FlightPlanner.MapControl.Markers.VORMarker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FlightPlanner.MapControl.Markers"
             mc:Ignorable="d"
             Height="20"
             Width="20"
             d:DesignHeight="20" d:DesignWidth="20">
    <Grid>
        <Polygon Fill="White" Stroke="Black" StrokeThickness="1.4" Points="5,0 15,0 20,8 15,16 5,16 0,8" Margin="0,2" />
        <Ellipse Fill="Black" StrokeThickness="1" Margin="8,8" />
    </Grid>
</UserControl>
